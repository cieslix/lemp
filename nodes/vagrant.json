{
    "run_list": [
        "recipe[php5_ppa::from_ondrej]",
        "recipe[php5-fpm::install]",
        "recipe[nginx]",
        "recipe[nginx-fastcgi]",
        "recipe[mariadb::server]",
        "recipe[mariadb::client]",
        "recipe[varnishd]",
        "recipe[cieslix]"
    ],
    "app": {
        "name": "vagrant",
        "web_dir": "/var/www/vagrant",
        "import_sql": "/vagrant/magento/magento_data_1.9.0.1.sql"
    },
    "user": {
        "name": "www-data",
        "group": "www-data"
    },
    "php_fpm": {
        "php_modules": [
            "php5-common",
            "php5-cli",
            "php5-mysql",
            "php5-curl",
            "php5-gd",
            "php5-mcrypt",
            "php-apc",
            "php5-xdebug"
        ]
    },
    "nginx": {
        "install_method": "source",
        "version": "1.7.7",
        "default_site_enabled": false
    },
    "mariadb": {
        "server_root_password": "password",
        "server_repl_password": "password",
        "server_debian_password": "password"
    },
    "automatic": {
        "ipaddress": "192.168.35.10"
    }
}
